{
  "version": 2,
  "builds": [
    {
      "src": "admin-nextjs/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/admin(.*)",
      "dest": "/admin$1"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
