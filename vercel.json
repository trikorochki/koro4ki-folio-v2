{
  "version": 2,
  "builds": [
    {
      "src": "admin-nextjs/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/api/listen",
      "dest": "/api/listen.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
