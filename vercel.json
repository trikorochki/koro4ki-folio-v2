{
  "version": 2,
  "builds": [
    {
      "src": "admin-nextjs/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/listen.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/stats.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/admin(.*)",
      "dest": "/admin$1"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin/$1"
    },
    {
      "src": "/api/(listen|stats)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
