{
  "version": 2,
  "builds": [

    {
      "src": "package.json",
      "use": "@vercel/next"
    },

    {
      "src": "api/listen.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/stats.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [

    {
      "src": "/admin((?!/api).*)",
      "dest": "/admin"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin/$1"
    },

    {
      "src": "/api/listen",
      "dest": "/api/listen.py"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.py"
    },

    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
