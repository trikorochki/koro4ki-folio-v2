{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" },
    { "src": "api/listen.py", "use": "@vercel/python" },
    { "src": "api/stats.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/listen", "dest": "/api/listen.py" },
    { "src": "/api/stats", "dest": "/api/stats.py" },
    {
      "src": "^/$",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/(.*\\.(css|js|ico|png|jpg|svg|mp3|woff2))$",
      "dest": "/$1"
    }
  ]
}
