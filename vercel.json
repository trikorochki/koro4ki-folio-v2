{
  "version": 2,
  "builds": [
    {
      "src": "api/listen.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/stats.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/check-location.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/listen",
      "dest": "/api/listen.py"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.py"
    },
    {
      "src": "/api/check-location",
      "dest": "/api/check-location.py"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}