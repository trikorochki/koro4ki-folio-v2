{
  "version": 2,
  "builds": [
    {
      "src": "api/check-location.py",
      "use": "@vercel/python"
    },  
    {
      "src": "api/listen.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/stats.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/check-location",
      "dest": "/api/check-location.py"
    },
    {
      "src": "/api/listen",
      "dest": "/api/listen.py"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.py"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    }
  ],
  "rewrites": [
    {
      "source": "/music/(.*)",
      "destination": "https://rpattpnro3om3v4l.public.blob.vercel-storage.com/music/$1"
    }
  ]
}
