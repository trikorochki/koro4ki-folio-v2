{
  "version": 2,
  "builds": [
    {
      "src": "api/listen.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    },
    {
      "src": "favicon.ico",
      "use": "@vercel/static"
    },
    {
      "src": "api/stats.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/listen",
      "dest": "api/listen.py"
    },
    {
      "src": "/api/stats",
      "dest": "api/stats.py"
    },
    { "src": "/admin", "dest": "/admin.html" }
  ]
}